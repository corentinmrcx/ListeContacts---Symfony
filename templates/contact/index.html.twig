{% extends 'base.html.twig' %}

{% block title %}Liste de contacts{% endblock %}

{% block body %}
    <h1>Liste de contacts</h1>
    <ul>
        {% for contact in contacts %}
            <li><a href="{{ path('app_contact_show', {id: contact.id}) }}">{{ contact.lastname }}, {{ contact.firstname }}</a></li>
        {% endfor %}
    </ul>

{% endblock %}